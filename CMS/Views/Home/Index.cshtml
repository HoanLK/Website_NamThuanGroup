@using CMS.Models.ViewModels
@model HomeViewModel

<!-- CatText -->
@helper catText(ComponentViewModel component)
{
    if (component != null)
    {
        <div class="col-md-6 col-lg-4 col-xl-3 p-0">
            <div class="card bg-primary-2 border-0 h-100 py-2">
                <div class="card-body appear-animation" data-appear-animation="fadeInRightShorter">
                    <span class="top-sub-title text-color-light opacity-5 text-uppercase">@Html.Raw(component.SubTitle)</span>
                    <h2 class="font-weight-bold text-color-light text-5 mb-3 text-uppercase">
                        @Html.Raw(component.MainTitle)
                    </h2>
                    <p class="text-color-light opacity-7 mb-0">
                        @Html.Raw(component.Content)
                    </p>
                </div>
            </div>
        </div>
    }
}

<!-- CatImage -->
@helper catImage(ComponentViewModel component, string text, string animateIn, string animateOut, double timeout)
{
    if (component != null)
    {
        <div class="col-md-6 col-lg-4 col-xl-3 p-0">
            <div class="owl-carousel owl-theme dots-style-2 nav-style-2" data-plugin-options="{'items': 1, 'dots': true, 'nav': false, 'animateIn': '@animateIn', 'animateOut': '@animateOut', 'autoplay': true, 'autoplayTimeout': @timeout, 'autoplayHoverPause': true}">
                @foreach (var image in component.ComponentImages)
                {
                    <div><img src="@image.Link" class="img-fluid" alt="@Html.Raw(text)" title="@Html.Raw(text)"></div>
                }
            </div>
        </div>
    }
}

