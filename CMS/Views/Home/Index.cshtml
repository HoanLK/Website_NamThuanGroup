<!-- Choose Us -->
<section class="appear-animation" data-appear-animation="fadeIn" data-appear-animation-duration="1s">
    <div class="container-fluid">
        <div class="row">
            @foreach (var component in Model.GioiThieuDacDiem.Components.OrderBy(p => p.SortOrder))
            {
                if (!string.IsNullOrEmpty(component.MainTitle))
                {
                    @catText(component)
                }
                else
                {
                    @catImage(component, component.MainTitle + " " + component.SubTitle, component.AnimateIn, component.AnimateOut, component.Timeout)
                }
            }
        </div>
    </div>
</section>

