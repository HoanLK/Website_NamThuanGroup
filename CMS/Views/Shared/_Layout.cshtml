<!DOCTYPE html>
<html lang="@Resources.Lang.ToString().ToLower()" ⚡="" amp-version="2003101714470" class="i-amphtml-singledoc i-amphtml-standalone">
<head>
    <!-- Basic -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Content-Language" content="VN">

    <title>@Html.Raw(ViewBag.title) | namthuangroup</title>
    <meta name="keywords" content="@Html.Raw(ViewBag.keywords)" />
    <meta name="description" content="@Html.Raw(ViewBag.description)" />
    <link rel="canonical" href="@Html.Raw(ViewBag.url)" />

    <meta name="language" content="vi" />
    <meta name="robots" content="archive,follow,imageindex,index,odp,snippet,translate" />
    <!--<meta name="revised" content="Sunday, July 18th, 2010, 5:15 pm" /> --><!-- Thời gian sửa đổi -->

    <meta name="url" content="@Html.Raw(ViewBag.url)" />
    <meta name="identifier-URL" content="@Html.Raw(ViewBag.url)" />
    <meta name="coverage" content="Worldwide" />
    <meta name="distribution" content="Global" />
    <meta name="rating" content="General" />
    <meta name="revisit-after" content="7 days" />
    <meta http-equiv="Expires" content="0" />

    <!-- Declare that your website is family-safe -->
    <meta name="target" content="all" />
    <meta name="audience" content="all" />
    <meta name="coverage" content="Worldwide" />
    <meta name="distribution" content="Global" />
    <meta name="rating" content="safe for kids" />

    <!-- OpenGraph Meta Tags -->
    <meta name="og:title" content="@Html.Raw(ViewBag.title)" />
    <meta name="og:url" content="@Html.Raw(ViewBag.url)" />
    <meta name="og:image" content="@Html.Raw(ViewBag.image)" />
    <meta name="og:image:secure_url" content="@Html.Raw(ViewBag.image)" />
    <meta name="og:description" content="@Html.Raw(ViewBag.description)" />

    <meta name="og:locale" content="vi_VN" />
    <meta name="og:type" content="website" />

    <!-- FIXED -->
    <meta name="og:postal-code" content="180000" />
    <meta name="og:country-name" content="VietNam" />

    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:description" content="@Html.Raw(ViewBag.description)" />
    <meta name="twitter:title" content="@Html.Raw(ViewBag.title)" />

    <meta name="geo.region" content="VN" />

    <!-- Apple Meta Tags -->
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="viewport" content="width = 320, initial-scale = 2.3, user-scalable = no" />
    <meta name="viewport" content="width = device-width" />
    <meta name="viewport" content="initial-scale = 1.0" />
    <meta name="viewport" content="initial-scale = 2.3, user-scalable = no" />

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, shrink-to-fit=no" />

    <!-- FIX META -->
    @Html.Action("HeaderFixed", "Layout")

    <!-- Web Fonts  -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:100,300,400,500,600,700,900%7COpen+Sans:300,400,600,700,800" rel="stylesheet" type="text/css" />

    <!-- Fontawesome -->
    @Styles.Render("~/fontawesome")

    <!-- Vendors -->
    @Styles.Render("~/vendors/css")

    <!-- Style -->
    @Styles.Render("~/theme/css")

    <!-- Modernizr -->
    @Scripts.Render("~/modernizr")

</head>
<body>
    <div class="body">
        <!-- HEADER -->
        @Html.Partial("_Header")

        <!-- MAIN -->
        <div role="main" class="main">
            @RenderBody()
        </div>

        <!-- FOOTER -->
        @Html.Action("Footer", "Layout")
    </div>

    <!-- [ SCRIPT ] -->
    <!-- JQuery -->
    <script src="~/Content/Vendors/jquery/jquery.min.js"></script>
    <script src="~/Content/Vendors/jquery.appear/jquery.appear.min.js"></script>
    <script src="~/Content/Vendors/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/Content/Vendors/jquery-cookie/jquery-cookie.min.js"></script>
    <script src="~/Content/Vendors/jquery.lazyload/jquery.lazyload.min.js"></script>
    @Scripts.Render("~/vendors/js")

    <!-- Vendor -->
    @Scripts.Render("~/vendors/js")

    <!-- Theme -->
    @Scripts.Render("~/theme/js")

    <!-- Custome -->
    <script src="~/Content/Frontend/js/custom.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
